{
  "name": "jord",
  "version": "0.0.0-development",
  "description": "A game engine for small web based games.",
  "main": "dist/jord.js",
  "scripts": {
    "test": "jest --coverage",
    "report-coverage": "cat ./coverage/lcov.info |  codecov",
    "prebuild": "rimraf dist",
    "build": "webpack",
    "commit": "git-cz",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rhmoller/jord.git"
  },
  "keywords": [
    "game",
    "engine",
    "gamedev",
    "webgl",
    "typescript"
  ],
  "author": "Rene Hangstrup MÃ¸ller <rhmoller@gmail.com> (http://giddyplanet.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rhmoller/jord/issues"
  },
  "homepage": "https://github.com/rhmoller/jord#readme",
  "devDependencies": {
    "@types/jest": "^18.1.1",
    "codecov.io": "^0.1.6",
    "commitizen": "^2.9.5",
    "cz-conventional-changelog": "^1.2.0",
    "jest": "^18.1.0",
    "rimraf": "^2.5.4",
    "semantic-release": "^6.3.2",
    "ts-loader": "^2.0.0",
    "tslint": "beta",
    "tslint-eslint-rules": "^3.3.0",
    "tslint-loader": "^3.4.2",
    "typescript": "^2.1.5",
    "webpack": "^2.2.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
    },
    "testRegex": "/test/.*\\.(ts|js)$"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
